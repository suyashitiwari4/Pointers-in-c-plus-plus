#include<iostream>
using namespace std;
int main(){
    int size;
    cout<<"Enter the number of elements:";
    cin>>size;
    int arr[size];
    cout<<"Enter "<<size<< " elements:";
    for (int i=0;i<size;i++){
        cin>>arr[i];
        
    }
    int *ptr= arr+size-1;
    for(int i=0;i<size;i++){
        cout<<*ptr<<" ";
        ptr--;
    }
    return 0;
}
/*Output:
Enter the number of elements:5
Enter 5 elements:10
20
30
40
50
50 40 30 20 10 

=== Code Execution Successful ===*/
